import java.util.Scanner;

class Main
{
 public static void main(String args[])
 {
     Scanner z=new Scanner(System.in);
     int a,b,c;
     a=z.nextInt();
     b=z.nextInt();
     c=z.nextInt();
     if(a>b && a>c)
        System.out.print("a is taller");
    else if(b>a && b>c)
        System.out.print("b is taller");
    else
        System.out.print("c is taller");
 }
}
